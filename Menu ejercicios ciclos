                                    SALIR = Convert.ToInt32(Console.ReadLine());
                                    break;
                                }
                                Console.WriteLine("\nLos multiplos de 5 hasta el numero digitado son");
                                for (MULTI = 5; NU >= MULTI; MULTI = MULTI + 5)
                                {
                                    Console.Write(MULTI + ", ");
                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 3:
                            {
                                //Leer un número entero y determinar si la suma de sus dígitos es también un número primo (Ejercicio 23)
                                //--------------------------------------------------------
                                //Valor de las variables
                                divisor = 1;
                                contador = 0;
                                acumulador = 0;
                                //Ejercicio
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Blue;
                                Console.WriteLine("Este programa sirve para leer un número entero y determinar si la suma de sus dígitos es también un número primo");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el numero");
                                numero = Convert.ToInt32(Console.ReadLine());
                                for (numero = numero + 0; numero < 0; numero = numero + 0)
                                {                                    
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    numero = Convert.ToInt32(Console.ReadLine());
                                }
                                for (numero = numero + 0; numero > 10; numero = numero + 0)
                                {
                                    digito = numero % 10;
                                    acumulador = acumulador + digito;
                                    numero = numero / 10;
                                }
                                acumulador = acumulador + numero;
                                for (divisor = 1; divisor <= acumulador; divisor++)
                                {
                                    if (acumulador % divisor == 0)
                                    {
                                        contador++;
                                    }
                                }
                                if (contador == 2)
                                {
                                    Console.WriteLine("\nLa suma de todos los digitos es " + acumulador + " y es primo");
                                }
                                else
                                {
                                    Console.WriteLine("\nLa suma de todos los digitos es " + acumulador + " y no es primo");
                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

