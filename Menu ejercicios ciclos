                            }
                            break;

                        case 9:
                            {
                                //Leer dos números enteros y determinar a cuánto es igual el producto mutuo del primer dígito de cada uno (EJERCICIO 35)
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Blue;
                                Console.WriteLine("Leer dos números enteros y determinar a cuánto es igual el producto mutuo del primer dígito e cada uno.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el primer numero");
                                numero1 = Convert.ToInt32(Console.ReadLine());
                                for (numero1 = numero1 + 0; numero1 < 10; numero1 = numero1 + 0)
                                {
                                    Console.Clear();
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    numero1 = Convert.ToInt32(Console.ReadLine());
                                }
                                for (numero1 = numero1 + 0; numero1 > 10; numero1 = numero1 + 0)
                                {
                                    Udigito1 = numero1 % 10;
                                    numero1 = numero1 / 10;
                                }
                                Console.WriteLine("Ingrese el segundo numero");
                                numero2 = Convert.ToInt32(Console.ReadLine());
                                for (numero2 = numero2 + 0; numero2 < 10; numero2 = numero2 + 0)
                                {
                                    Console.Clear();
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    numero2 = Convert.ToInt32(Console.ReadLine());
                                }
                                for (numero2 = numero2 + 0; numero2 > 10; numero2 = numero2 + 0)
                                {
                                    Udigito2 = numero2 % 10;
                                    numero2 = numero2 / 10;
                                }
                                Console.WriteLine("El primer digito de el primer numero es " + numero1);
                                Console.WriteLine("El primer digito de el segundo numero es " + numero2);
                                Console.WriteLine("El producto mutuo de ambos digitos es " + (numero1 * numero2));
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 10:
                            {
                                //VALOR VARIABLE
                                numero = 5;
                                //Ejercicio
