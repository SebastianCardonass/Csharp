                        case 8:
                            {
                                //Generar los números del 1 al 10 utilizando un ciclo que vaya de 10 a 1 (Ejercicio 34)
                                //VALOR VARIABLES
                                nu = 11;
                                resta = 10;
                                //EJERCICIOS
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Blue;
                                Console.WriteLine("Este programa sirve para generar los números del 1 al 10 utilizando un ciclo que vaya de 10 a 1");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                rresta = nu - resta;
                                for (resta = 10; rresta < 10 && rresta >= 1; resta--)
                                {
                                    rresta = nu - resta;
                                    Console.Write(rresta + ", ");                                    

                                }                                
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
