                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Digite los numeros");
                                    NU = Convert.ToDouble(Console.ReadLine());
                                }
                                UDT = NU % 10;
                                for (NU = NU + 0; NU != 0; UDT = NU + 0)
                                {
                                    if (NU > 0 && UDT != 5)
                                    {
                                        Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                        NU = Convert.ToDouble(Console.ReadLine());
                                        for (NU = NU + 0; NU < 0; NU = NU + 0)
                                        {
                                            Console.ForegroundColor = ConsoleColor.DarkRed;
                                            Console.WriteLine("El numero debe de ser positivo");
                                            Console.WriteLine("-------------------------");
                                            Console.ForegroundColor = ConsoleColor.White;
                                            Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                            NU = Convert.ToDouble(Console.ReadLine());
                                        }
                                        UDT = NU % 10;

                                    }
                                    if (UDT == 5)
                                    {
                                        ACUMULADOR = ACUMULADOR + NU;
                                        CONTADOR++;
                                        Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                        NU = Convert.ToDouble(Console.ReadLine());
                                        for (NU = NU + 0; NU < 0; NU = NU + 0)
