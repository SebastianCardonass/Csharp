                        case 11:
                            {
                                //VALOR VARIABLE
                                multiplicador = 1;
                                numero = 1;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Blue;
                                Console.WriteLine("Este programa sirve para mostrar en pantalla las tablas de multiplicar del 1 al 10.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.Write("Presione ENTER para continuar con el programa");
                                Console.WriteLine("\n-------------------------");
                                Console.ReadLine();
                                for (numero = 1; numero <= 10; numero++)
                                {
                                    Console.WriteLine("\nTabla del " + numero + "\n");
                                    for (multiplicador = 1; multiplicador <= 10; multiplicador++)
                                    {
                                        Console.WriteLine(numero + " x " + multiplicador + " = " + (numero * multiplicador));
                                        
                                    }
                                    Console.WriteLine("");
                                    multiplicador = 1;

                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 12:
                            {
                                //leer un número entero y mostrar en pantalla su tabla de multiplicar (EJERCICIO 38)
                                //VALOR VARIABLES
                                multiplicador = 1;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Blue;
                                Console.WriteLine("Este programa sirve para leer un número entero y mostrar en pantalla su tabla de multiplicar.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el numero del cual desea saber la tabla");
                                numero = Convert.ToInt32(Console.ReadLine());
                                for (numero = numero + 0; numero < 0; numero = numero + 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("\nIngrese el numero del cual desea saber la tabla");
                                    numero = Convert.ToInt32(Console.ReadLine());
                                }
                                Console.WriteLine("\nIngrese hasta que multiplicador desea saber la tabla");
                                limite = Convert.ToInt32(Console.ReadLine());
                                for (limite = limite + 0; limite < 0; limite = limite + 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("\nIngrese hasta que multiplicador desea saber la tabla\n");
                                    limite = Convert.ToInt32(Console.ReadLine());
                                }
                                Console.WriteLine("\nLa tabla del " + numero + " hasta el " + limite + "\n");
                                for (multiplicador = 1; multiplicador <= limite; multiplicador++)
                                {
                                    Console.WriteLine(numero + " x " + multiplicador + " = " + (numero * multiplicador));                                    
                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 13:
                            {
                                //Utilizando el concepto de ciclo generar la serie de Fibonacci hasta llegar o sobrepasas el número 10000 (EJERCICIO 39)
                                //VALOR VARIABLES
                                numero1 = 1;
                                numero2 = 1;
                                numero3 = 0;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Blue;
                                Console.WriteLine("Utilizando el concepto de ciclo generar la serie de Fibonacci hasta llegar o sobrepasas el número 10000.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Presione ENTER para continuar con el programa");
                                Console.WriteLine("\n-------------------------");
                                Console.ReadLine();
                                Console.WriteLine("La serie de Fibonacci es\n");
                                Console.Write("0, 1, 1, ");
                                for (numero1 = numero1 + 0; numero3 <= 10000 || numero1 <= 10000 || numero2 <= 10000; numero1 = numero1 + 0)
                                {

                                    numero3 = numero1 + numero2;
                                    Console.Write(numero3 + ", ");
                                    if (numero3 >= 10000)
                                    {
                                        break;
                                    }
                                    numero1 = numero2 + numero3;
                                    Console.Write(numero1 + ", ");
                                    if (numero1 >= 10000)
                                    {
                                        break;
                                    }
                                    numero2 = numero3 + numero1;
                                    Console.Write(numero2 + ", ");
                                    if (numero2 >= 10000)
                                    {
                                        break;
                                    }
                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 14:
                            {
                                //Determinar cuántos elementos de la serie de Fibonacci se encuentran entre 1000 y 2000 (EJERCICIO 43)
                                //VALOR VARIABLES
                                numero1 = 1;
                                numero2 = 1;
                                numero3 = 0;
                                contador = 0;
                                acumulador = 0;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Blue;
                                Console.WriteLine("Este programa sirve para determinar cuántos elementos de la serie de Fibonacci se encuentran entre 1000 y 2000.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.Write("Presione ENTER para continuar con el programa");
                                Console.WriteLine("\n-------------------------");
                                Console.ReadLine();
                                Console.WriteLine("La serie de Fibonacci es\n");
                                Console.Write("0, 1, 1, ");
                                for (numero1 = 1; numero3 <= 10000 || numero1 <= 10000 || numero2 <= 10000; numero1 = numero1 + 0)
                                {

                                    numero3 = numero1 + numero2;
                                    Console.Write(numero3 + ", ");
                                    if (numero3 > 1000 && numero3 < 2000)
                                    {
                                        contador++;
                                        acumulador = acumulador + numero3;
                                    }

                                    numero1 = numero2 + numero3;
                                    Console.Write(numero1 + ", ");
                                    if (numero1 > 1000 && numero1 < 2000)
                                    {
                                        contador++;
                                        acumulador = acumulador + numero1;
                                    }

                                    numero2 = numero3 + numero1;
                                    Console.Write(numero2 + ", ");
                                    if (numero2 > 1000 && numero2 < 2000)
                                    {
                                        contador++;
                                        acumulador = acumulador + numero2;
                                    }

                                }
                                Console.WriteLine("\n\nEl numero de elementos que hay en la serie de Fibonacci entre 1000 y 2000 es " + contador + " elemento y es el numero " + acumulador);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 15:
                            {
                                SALIR = 0;
                            }
                            break;

                        default:
                            {
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Red;
                                Console.WriteLine("El numero del menu van de 0 a 15 idiota");
                                Console.WriteLine("\n-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("\nSi desea volver al menu de (do while) ingrese cualquier numero sino ingrese 0");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;
                    }
                } while (SALIR != 0);
            }
            break;
        case 3:
            {
                //MENU DO WHILE
                do
                {
                    Console.Clear();
                    Console.WriteLine("Menu ejercicios con (Do While) ");
                    Console.WriteLine("\n1. Ejercicio 3");
                    Console.WriteLine("2. Ejercicio 13");
                    Console.WriteLine("3. Ejercicio 23");
                    Console.WriteLine("4. Ejercicio 30");
                    Console.WriteLine("5. Ejercicio 31");
                    Console.WriteLine("6. Ejercicio 32");
                    Console.WriteLine("7. Ejercicio 33");
                    Console.WriteLine("8. Ejercicio 34");
                    Console.WriteLine("9. Ejercicio 35");
                    Console.WriteLine("10. Ejercicio 36");
                    Console.WriteLine("11. Ejercicio 37");
                    Console.WriteLine("12. Ejercicio 38");
                    Console.WriteLine("13. Ejercicio 39");
                    Console.WriteLine("14. Ejercicio 43");
                    Console.WriteLine("15. Volver");
                    Console.WriteLine("\nIngrese la opcion del ejercicio que desea ver");
                    OPCION3 = 0;
                    OPCION3 = Convert.ToDouble(Console.ReadLine());
                    switch (OPCION3)
                    {
                        case 1:
                            {
                                //Leer un número entero y mostrar todos los divisores exactos del número comprendidos entre 1 y el número leído.
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para leer un número entero y mostrar todos los divisores exactos del número comprendidos entre 1 y el número leído.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el numero");
                                NU = Convert.ToDouble(Console.ReadLine());
                                DIV = 1;
                                while (NU < 0) 
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    NU = Convert.ToDouble(Console.ReadLine());
                                } 
                                Console.WriteLine("\nLos divisores exactos del numero ingresado son ");
                                do
                                {
                                    EXAC = NU % DIV;
                                    if (EXAC == 0)
                                    {
                                        Console.Write(DIV + ", ");
                                        DIV = DIV + 1;
                                    }
                                    else
                                    {
                                        DIV = DIV + 1;
                                    }
                                } while (NU >= DIV);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 2:
                            {
                                // leer un entero y mostrar todos los múltiplos de 5 comprendidos entre 1 y el número leído (Ejercicio 13)
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para leer un entero y mostrar todos los múltiplos de 5 comprendidos entre 1 y el número leído.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el numero");
                                NU = Convert.ToDouble(Console.ReadLine());
                                MULTI = 5;
                                while (NU < 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    NU = Convert.ToDouble(Console.ReadLine());
                                }
                                if (NU <= 4)
                                {
                                    Console.WriteLine("No hay multiplos de 5 entre 1 y el numero ingresado");
                                    Console.WriteLine("\n-------------------------");
                                    Console.Write("\nPresione ENTER para continuar");
                                    Console.ReadLine();
                                    Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                    SALIR = Convert.ToInt32(Console.ReadLine());
                                    break;
                                }
                                Console.WriteLine("\nLos multiplos de 5 hasta el numero digitado son");
                                do
                                {
                                    Console.Write(MULTI + ", ");
                                    MULTI = MULTI + 5;

                                } while (NU >= MULTI);  
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 3:
                            {
                                //Leer un número entero y determinar si la suma de sus dígitos es también un número primo (Ejercicio 23)
                                //--------------------------------------------------------
                                //Valor de las variables
                                divisor = 1;
                                contador = 0;
                                acumulador = 0;
                                //Ejercicio
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para leer un número entero y determinar si la suma de sus dígitos es también un número primo");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el numero");
                                numero = Convert.ToInt32(Console.ReadLine());
                                while (numero < 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    numero = Convert.ToInt32(Console.ReadLine());
                                }
                                do
                                {
                                    digito = numero % 10;
                                    acumulador = acumulador + digito;
                                    numero = numero / 10;
                                } while (numero > 10);

                                acumulador = acumulador + numero;

                                do
                                {   if (acumulador % divisor == 0)
                                        {
                                            contador++;
                                        }
                                    divisor++;

                                } while (divisor <= acumulador);
                                if (contador == 2)
                                {
                                    Console.WriteLine("\nLa suma de todos los digitos es " + acumulador + " y es primo");
                                }
                                else
                                {
                                    Console.WriteLine("\nLa suma de todos los digitos es " + acumulador + " y no es primo");
                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 4:
                            {
                                //Leer un número entero y mostrar todos sus componentes numéricos o sea aquellos quienes el sea un múltiplo (Ejercicio 30)
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para leer un número entero y mostrar todos sus componentes numéricos o sea aquellos para\r\nquienes el sea un múltiplo.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el numero");
                                NU = Convert.ToDouble(Console.ReadLine());
                                DIV = 1;
                                while (NU < 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    NU = Convert.ToDouble(Console.ReadLine());
                                }
                                Console.WriteLine("\nLos numeros que tiene como multiplo " + NU + " son");
                                do
                                {
                                    EXAC = NU % DIV;
                                    if (EXAC == 0)
                                    {
                                        Console.Write(DIV + ", ");
                                        DIV = DIV + 1;
                                    }
                                    else
                                    {
                                        DIV = DIV + 1;
                                    }
                                } while (NU >= DIV);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 5:
                            {
                                //Leer números hasta que digiten 0 y determinar a cuánto es igual el promedio de los números terminados en 5 (Ejercicio 31)
                                // VALOR VARIABLE
                                ACUMULADOR = 0;
                                CONTADOR = 0;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para leer números hasta que digiten 0 y determinar a cuánto es igual el promedio de los números terminados en 5");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Digite los numeros");
                                NU = Convert.ToDouble(Console.ReadLine());
                                while (NU < 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Digite los numeros");
                                    NU = Convert.ToDouble(Console.ReadLine());
                                }
                                UDT = NU % 10;
                                do
                                {
                                    if (NU > 0 && UDT != 5)
                                    {
                                        Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                        NU = Convert.ToDouble(Console.ReadLine());
                                        while (NU < 0)
                                        {
                                            Console.ForegroundColor = ConsoleColor.DarkRed;
                                            Console.WriteLine("El numero debe de ser positivo");
                                            Console.WriteLine("-------------------------");
                                            Console.ForegroundColor = ConsoleColor.White;
                                            Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                            NU = Convert.ToDouble(Console.ReadLine());
                                        }
                                        UDT = NU % 10;

                                    }
                                    if (UDT == 5)
                                    {
                                        ACUMULADOR = ACUMULADOR + NU;
                                        CONTADOR++;
                                        Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                        NU = Convert.ToDouble(Console.ReadLine());
                                        while (NU < 0)
                                        {
                                            Console.ForegroundColor = ConsoleColor.DarkRed;
                                            Console.WriteLine("El numero debe de ser positivo");
                                            Console.WriteLine("-------------------------");
                                            Console.ForegroundColor = ConsoleColor.White;
                                            Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                            NU = Convert.ToDouble(Console.ReadLine());
                                        }
                                        UDT = NU % 10;

                                    }
                                } while (NU != 0);
                                if (ACUMULADOR != 0 && CONTADOR != 0)
                                {
                                    PROMEDIO = ACUMULADOR / CONTADOR;
                                    Console.WriteLine("\nEl promedio es de " + PROMEDIO);
                                }
                                else
                                {
                                    Console.WriteLine("\nNo se ingreso ningun numero que tuviera como ultimo digito el 5");
                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 6:
                            {
                                //leer números hasta que digiten 0 y determinar a cuanto es igual el promedio entero de los número primos leídos. (Ejercicio 32)
                                // VALOR VARIABLE
                                acumuladorsuma = 0;
                                contadorsuma = 0;
                                contadorprimo = 0;
                                divisor = 1;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para leer números hasta que digiten 0 y determinar a cuanto es igual el promedio entero de los número primos leídos");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Digite los numeros");
                                numero = Convert.ToInt32(Console.ReadLine());
                                while (numero < 0 || numero == 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo y el primer numero debe ser diferente a 0");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Digite los numeros");
                                    numero = Convert.ToInt32(Console.ReadLine());
                                }
                                do
                                {
                                    if (numero % divisor == 0)
                                    {
                                        contadorprimo++;

                                    }
                                    divisor++;
                                } while (divisor <= numero);
                                if (contadorprimo == 2)
                                {
                                    acumuladorsuma = acumuladorsuma + numero;
                                    contadorsuma++;

                                }
                                do
                                {
                                    Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                    numero = Convert.ToInt32(Console.ReadLine());
                                    while (numero < 0)
                                    {
                                        Console.ForegroundColor = ConsoleColor.DarkRed;
                                        Console.WriteLine("El numero debe de ser positivo");
                                        Console.WriteLine("-------------------------");
                                        Console.ForegroundColor = ConsoleColor.White;
                                        Console.WriteLine("Digite el siguiente numero o digite el numero 0 si ya desea sacar el promedio");
                                        numero = Convert.ToInt32(Console.ReadLine());
                                    }
                                    divisor = 1;
                                    contadorprimo = 0;
                                    do
                                    {
                                        if (numero % divisor == 0)
                                        {
                                            contadorprimo++;
                                        }
                                        divisor++;
                                    } while (divisor <= numero);
                                    if (contadorprimo == 2)
                                    {
                                        acumuladorsuma = acumuladorsuma + numero;
                                        contadorsuma++;
                                    }

                                } while (numero != 0);
                                if (acumuladorsuma != 0 && contadorsuma != 0)
                                {
                                    promedio = acumuladorsuma / contadorsuma;
                                    Console.WriteLine("\nEl promedio de los numeros primos es de " + promedio);
                                }
                                else
                                {
                                    Console.WriteLine("\nNo se ingreso ningun numero primo");
                                }
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 7:
                            {
                                //Si 32768 es el tope superior para los números entero cortos, determinar cuál es el número primo mas cercano por debajo de él (Ejercicio 33)
                                //VALOR VARIABLES
                                numero = 32767;
                                contador = 0;
                                divisor = 1;
                                contadorpar = 0;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para saber el numero primo mas cercano por debajo de 32768");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                do
                                {
                                    contador = 0;
                                    divisor = 1;
                                    do
                                    {
                                        if (numero % divisor == 0)
                                        {
                                            contador++;
                                        }
                                        divisor++;

                                    } while (divisor <= numero);


                                    if (contador == 2)
                                    {
                                        contadorpar++;
                                        divisor = numero;
                                    }
                                    numero -= 1;

                                } while (contadorpar != 1);
                                Console.WriteLine("El numero primo mas cercano por debajo de 32768 es " + divisor);

                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 8:
                            {
                                //Generar los números del 1 al 10 utilizando un ciclo que vaya de 10 a 1 (Ejercicio 34)
                                //VALOR VARIABLES
                                nu = 11;
                                resta = 10;
                                //EJERCICIOS
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para generar los números del 1 al 10 utilizando un ciclo que vaya de 10 a 1");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                rresta = nu - resta;
                                do
                                {
                                    rresta = nu - resta;
                                    Console.Write(rresta + ", ");
                                    resta -= 1;

                                } while (rresta < 10 && rresta >= 1);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 9:
                            {
                                //Leer dos números enteros y determinar a cuánto es igual el producto mutuo del primer dígito de cada uno (EJERCICIO 35)
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Leer dos números enteros y determinar a cuánto es igual el producto mutuo del primer dígito e cada uno.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el primer numero");
                                numero1 = Convert.ToInt32(Console.ReadLine());
                                while (numero1 < 0)
                                {
                                    Console.Clear();
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    numero1 = Convert.ToInt32(Console.ReadLine());
                                }
                                do
                                {
                                    Udigito1 = numero1 % 10;
                                    numero1 = numero1 / 10;

                                } while (numero1 > 10);

                                Console.WriteLine("Ingrese el segundo numero");
                                numero2 = Convert.ToInt32(Console.ReadLine());
                                while (numero2 < 0)
                                {
                                    Console.Clear();
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("Ingrese el numero");
                                    numero2 = Convert.ToInt32(Console.ReadLine());
                                }
                                do
                                {
                                    Udigito2 = numero2 % 10;
                                    numero2 = numero2 / 10;

                                } while (numero2 > 10);
                                Console.WriteLine("El primer digito de el primer numero es " + numero1);
                                Console.WriteLine("El primer digito de el segundo numero es " + numero2);
                                Console.WriteLine("El producto mutuo de ambos digitos es " + (numero1 * numero2));
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 10:
                            {
                                //VALOR VARIABLE
                                numero = 5;
                                multiplicador = 1;
                                //Ejercicio
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para mostrar en pantalla la tabla de multiplicar del número 5.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.Write("Presione ENTER para continuar con el programa");
                                Console.WriteLine("\n-------------------------");
                                Console.ReadLine();
                                Console.Write("La tabla de multiplicar del 5 \n");
                                do
                                {
                                    Console.WriteLine(numero + " x " + multiplicador + " = " + (numero * multiplicador));
                                    multiplicador++;

                                } while (multiplicador <= 10);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (For) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                                
                            }
                            break;

                        case 11:
                            {
                                //VALOR VARIABLE
                                multiplicador = 1;
                                numero = 1;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para mostrar en pantalla las tablas de multiplicar del 1 al 10.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.Write("Presione ENTER para continuar con el programa");
                                Console.WriteLine("\n-------------------------");
                                Console.ReadLine();
                                do
                                {
                                    Console.WriteLine("\nTabla del " + numero + "\n");
                                    do
                                    {
                                        Console.WriteLine(numero + " x " + multiplicador + " = " + (numero * multiplicador));
                                        multiplicador++;
                                    }while (multiplicador <= 10) ;
                                    Console.WriteLine("");
                                    numero++;
                                    multiplicador = 1;

                                }while (numero <= 10);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 12:
                            {
                                //VALOR VARIABLES
                                multiplicador = 1;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para leer un número entero y mostrar en pantalla su tabla de multiplicar.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Ingrese el numero del cual desea saber la tabla");
                                numero = Convert.ToInt32(Console.ReadLine());
                                while (numero < 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("\nIngrese el numero del cual desea saber la tabla");
                                    numero = Convert.ToInt32(Console.ReadLine());
                                }
                                Console.WriteLine("\nIngrese hasta que multiplicador desea saber la tabla");
                                limite = Convert.ToInt32(Console.ReadLine());
                                while (limite < 0)
                                {
                                    Console.ForegroundColor = ConsoleColor.DarkRed;
                                    Console.WriteLine("El numero debe de ser positivo");
                                    Console.WriteLine("-------------------------");
                                    Console.ForegroundColor = ConsoleColor.White;
                                    Console.WriteLine("\nIngrese hasta que multiplicador desea saber la tabla\n");
                                    limite = Convert.ToInt32(Console.ReadLine());
                                }
                                Console.WriteLine("\nLa tabla del " + numero + " hasta el " + limite + "\n");
                                do
                                {
                                    Console.WriteLine(numero + " x " + multiplicador + " = " + (numero * multiplicador));
                                    multiplicador++;

                                } while (multiplicador <= limite);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 13:
                            {
                                //Utilizando el concepto de ciclo generar la serie de Fibonacci hasta llegar o sobrepasas el número 10000 (EJERCICIO 39)
                                //VALOR VARIABLES
                                numero1 = 1;
                                numero2 = 1;
                                numero3 = 0;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Utilizando el concepto de ciclo generar la serie de Fibonacci hasta llegar o sobrepasas el número 10000.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("Presione ENTER para continuar con el programa");
                                Console.WriteLine("\n-------------------------");
                                Console.ReadLine();
                                Console.WriteLine("La serie de Fibonacci es\n");
                                Console.Write("0, 1, 1, ");
                                do 
                                {

                                    numero3 = numero1 + numero2;
                                    Console.Write(numero3 + ", ");
                                    if (numero3 >= 10000)
                                    {
                                        break;
                                    }
                                    numero1 = numero2 + numero3;
                                    Console.Write(numero1 + ", ");
                                    if (numero1 >= 10000)
                                    {
                                        break;
                                    }
                                    numero2 = numero3 + numero1;
                                    Console.Write(numero2 + ", ");
                                    if (numero2 >= 10000)
                                    {
                                        break;
                                    }
                                }while (numero3 <= 10000 || numero1 <= 10000 || numero2 <= 10000);

                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 14:
                            {
                                //Determinar cuántos elementos de la serie de Fibonacci se encuentran entre 1000 y 2000.
                                //VALOR VARIABLES
                                numero1 = 1;
                                numero2 = 1;
                                numero3 = 0;
                                contador = 0;
                                acumulador = 0;
                                //EJERCICIO
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.DarkYellow;
                                Console.WriteLine("Este programa sirve para determinar cuántos elementos de la serie de Fibonacci se encuentran entre 1000 y 2000.");
                                Console.WriteLine("-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.Write("Presione ENTER para continuar con el programa");
                                Console.WriteLine("\n-------------------------");
                                Console.ReadLine();
                                Console.WriteLine("La serie de Fibonacci es\n");
                                Console.Write("0, 1, 1, ");
                                do
                                {

                                    numero3 = numero1 + numero2;
                                    Console.Write(numero3 + ", ");
                                    if (numero3 > 1000 && numero3 < 2000)
                                    {
                                        contador++;
                                        acumulador = acumulador + numero3;
                                    }

                                    numero1 = numero2 + numero3;
                                    Console.Write(numero1 + ", ");
                                    if (numero1 > 1000 && numero1 < 2000)
                                    {
                                        contador++;
                                        acumulador = acumulador + numero1;
                                    }

                                    numero2 = numero3 + numero1;
                                    Console.Write(numero2 + ", ");
                                    if (numero2 > 1000 && numero2 < 2000)
                                    {
                                        contador++;
                                        acumulador = acumulador + numero2;
                                    }

                                } while (numero3 <= 10000 || numero1 <= 10000 || numero2 <= 10000);
                                Console.WriteLine("\n\nEl numero de elementos que hay en la serie de Fibonacci entre 1000 y 2000 es " + contador + " elemento y es el numero " + acumulador);
                                Console.WriteLine("\n-------------------------");
                                Console.Write("\nPresione ENTER para continuar");
                                Console.ReadLine();
                                Console.WriteLine("\nPara salir al menu principal ingrese 0, Para seguir usando los ejercicios con el ciclo (Do While) presione cualquier numero");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;

                        case 15:
                            {
                                SALIR = 0;
                            }
                            break;

                        default:
                            {
                                Console.Clear();
                                Console.ForegroundColor = ConsoleColor.Red;
                                Console.WriteLine("El numero del menu van de 0 a 15 idiota");
                                Console.WriteLine("\n-------------------------");
                                Console.ForegroundColor = ConsoleColor.White;
                                Console.WriteLine("\nSi desea volver al menu de (do while) ingrese cualquier numero sino ingrese 0");
                                SALIR = Convert.ToInt32(Console.ReadLine());
                            }
                            break;
                    }
                } while (SALIR != 0);

            }
            break;
        case 4:
            {
                Console.Clear();
                Console.WriteLine("Si esta seguro de que desea salir ingrese 0, si quiere seguir en el menu ingrese cualquier numero");
                SALIR = Convert.ToInt32(Console.ReadLine());
                if (SALIR == 0)
                {
                    Environment.Exit(0);
                }
                
            }   
            break;
        default:
            {
                Console.Clear();
                Console.ForegroundColor = ConsoleColor.Red;
                Console.WriteLine("El numero debe de ser entre 0 y 4 idiota");
                Console.WriteLine("\n-------------------------");
                Console.ForegroundColor= ConsoleColor.White;
                Console.WriteLine("\nSi desea seguir ingrese cualquier numero sino ingrese 5");
                OPCION = Convert.ToInt32(Console.ReadLine());
            }
            break;
    }
      
} while (OPCION != 5);

